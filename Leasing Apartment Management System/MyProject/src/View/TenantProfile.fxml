<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" styleClass="background" stylesheets="@testtheme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TenantPageController">

   <GridPane layoutX="14.0" layoutY="60.0" prefHeight="180.0" prefWidth="244.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="117.0" minWidth="10.0" prefWidth="98.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="146.0" minWidth="10.0" prefWidth="146.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Label text="User ID" />
         <Label text="Name" GridPane.rowIndex="1" />
         <Label text="Email ID" GridPane.rowIndex="2" />
         <Label text="Contact No." GridPane.rowIndex="3" />
         <Label text="Passport No." GridPane.rowIndex="4" />
         <TextField fx:id="showid" editable="false" prefHeight="25.0" prefWidth="125.0" GridPane.columnIndex="1">
            <font>
               <Font name="System" size="12.0" />
            </font>
         </TextField>
         <TextField fx:id="showname" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <TextField fx:id="showemail" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         <TextField fx:id="showph" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         <TextField fx:id="showpassport" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      </children>
   </GridPane>
   <Button fx:id="Update" layoutX="14.0" layoutY="261.0" mnemonicParsing="false" onAction="#doUpdate" stylesheets="@testtheme.css" text="Update" />
   <Button fx:id="Save" disable="true" layoutX="110.0" layoutY="261.0" mnemonicParsing="false" onAction="#doSave" stylesheets="@testtheme.css" text="Save" />

	<!-- TODO Add Nodes -->
</AnchorPane>
